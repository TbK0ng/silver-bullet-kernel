{
  "name": "go",
  "priority": 70,
  "detect": {
    "anyOfFiles": [
      "go.mod"
    ]
  },
  "implementation": {
    "pathPrefixes": [
      "cmd/",
      "internal/",
      "pkg/",
      "tests/",
      "scripts/"
    ],
    "pathFiles": [
      "go.mod",
      "go.sum"
    ]
  },
  "verify": {
    "fast": [
      "go test ./..."
    ],
    "full": [
      "go test ./..."
    ],
    "ci": [
      "go test ./..."
    ]
  },
  "capabilities": {
    "semantic": {
      "rename": {
        "supported": true,
        "backend": "symbol-index",
        "deterministic": true
      },
      "referenceMap": {
        "supported": true,
        "backend": "symbol-index",
        "deterministic": true
      },
      "safeDeleteCandidates": {
        "supported": true,
        "backend": "symbol-index",
        "deterministic": true
      }
    }
  },
  "sdk": {
    "source": "builtin",
    "validated": true,
    "validationStatus": "certified"
  }
}
